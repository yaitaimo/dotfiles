[user]
	name = Yu Takabatake
	email = manofslug@gmail.com
[core]
	excludesfile = /Users/yuuair/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[color]
	ui = true
[alias]
  lg = log --oneline --graph --decorate
  co = checkout
  cm = commit -am
  st = status
  br = branch
  ba = branch -a
  ft = fetch
  fp = fetch -p


